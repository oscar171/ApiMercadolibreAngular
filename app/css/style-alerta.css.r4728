.alerta{
	width: 100%;
	background-color: #6BAF79;
    color: white;
    font-style: oblique;
    font-family: monospace;
	margin: 0px auto;
	text-align: center;
	display: none;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.alerta-error{
	background-color: #FF412E;
}